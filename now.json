{
  "public": false,
  "name": "opencollective-bot",
  "version": 2,
  "alias": "opencollective-bot.now.sh",
  "builds": [{ "src": "src/index.ts", "use": "@now/node-server@canary" }],
  "env": {
    "APP_ID": "@ocb-app-id",
    "WEBHOOK_SECRET": "@ocb-webhook-secret",
    "PRIVATE_KEY": "@ocb-private-key"
  },
  "github": {
    "enabled": true,
    "autoAlias": false
  }
}
